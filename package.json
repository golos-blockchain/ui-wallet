    "whatwg-fetch": "^0.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-proposal-function-sent": "^7.18.6",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@red-mobile/cordova-plugin-shortcuts-android": "^1.0.1",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/router": "^6.5.14",
    "babel-plugin-styled-components": "^1.5.1",
    "chai": "^4.1.2",
    "cheerio": "1.0.0-rc.10",
    "cordova-android": "^10.1.2",
    "cordova-config": "^0.7.0",
    "cordova-plugin-advanced-http": "^3.3.1",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-backbutton": "^0.3.0",
    "cordova-plugin-badge": "^0.8.8",
    "cordova-plugin-device": "^2.1.0",
    "cordova-plugin-file": "^7.0.0",
    "cordova-plugin-native-logs": "^1.0.5",
    "cordova-plugin-splashscreen": "^6.0.1",
    "core-js": "^3.19.1",
    "cross-env": "^7.0.3",
    "folder-hash": "^4.0.4",
    "gls-wallet-native-core": "file:native_core",
    "jsdom": "^11.11.0",
    "mocha": "^10.1.0",
    "node-watch": "^0.7.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sinon": "^1.17.7",
    "terser-webpack-plugin": "^5.3.6",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.10.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "gitDependencies": {
    "formik": "https://github.com/golos-blockchain/formik/tree/master/packages/formik#3b21166c33ade760d562091e1fa0b71d172a7aaf",
    "react-foundation-components": "https://github.com/golos-blockchain/react-foundation-components#ff2e48fd5ec1b587899bd67d686d0889da4e4ab1"
  },
  "engines": {
    "node": ">=8.11.3",
    "npm": ">=5.6.0"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4,
    "semi": true,
    "trailingComma": "es5"
  },
  "build": {
    "productName": "GOLOS Блоги",
    "appId": "gls.blogs",
    "files": [
      "dist/electron",
      "!node_modules"
    ],
    "win": {
      "executableName": "glsblogs",
      "artifactName": "glsblogs-${version}.${ext}",
      "icon": "electron/icons/256x256.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "executableName": "glsblogs",
      "artifactName": "glsblogs-${version}.${ext}",
      "category": "Finance",
      "icon": "electron/icons",
      "target": [
        "deb"
      ]
    },
    "directories": {
      "output": "dist/electron-binaries",
      "buildResources": "electron"
    },
    "npmRebuild": false
  },
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {
      "cordova-plugin-advanced-http": {
        "ANDROIDBLACKLISTSECURESOCKETPROTOCOLS": "SSLv3,TLSv1"
      },
      "cordova-plugin-androidx-adapter": {},
      "cordova-plugin-backbutton": {},
      "cordova-plugin-native-logs": {},
      "cordova-plugin-splashscreen": {},
      "@red-mobile/cordova-plugin-shortcuts-android": {
        "ANDROIDX_CORE_VERSION": "1.3.2"
      },
      "gls-wallet-native-core": {}
    }
  }
}
