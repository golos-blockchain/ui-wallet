    "npm": ">=5.6.0"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4,
    "semi": true,
    "trailingComma": "es5"
  },
  "build": {
    "productName": "GOLOS Блоги",
    "appId": "gls.blogs",
    "files": [
      "dist/electron",
      "!node_modules"
    ],
    "win": {
      "executableName": "glsblogs",
      "artifactName": "glsblogs-${version}.${ext}",
      "icon": "electron/icons/256x256.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "executableName": "glsblogs",
      "artifactName": "glsblogs-${version}.${ext}",
      "category": "Finance",
      "icon": "electron/icons",
      "target": [
        "deb"
      ]
    },
    "directories": {
      "output": "dist/electron-binaries",
      "buildResources": "electron"
    },
    "npmRebuild": false
  }
}
